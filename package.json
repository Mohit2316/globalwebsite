{
  "name": "echtrade-ui",
  "version": "0.0.1",
  "keywords": [
    "React",
    "Material-ui"
  ],
  "scripts": {
    "build": "webpack --mode=build",
    "dev": "webpack --progress --colors --mode=dev",
    "start": "webpack-dev-server --config webpack-local.config.js --mode=dev",
    "start-prod": "webpack-dev-server --config webpack-prod.config.js",
    "demo-prod": "webpack-dev-server --config webpack-demo.config.js --mode=prod",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "dependencies": {
    "lodash": "^4.17.4",
