    "react-dom": "^15.5.0",
    "react-spinners": "^0.2.5"
  },
  "peerDependencies": {
    "react": "~0.14.8 || ^15.0.0",
    "react-dom": "~0.14.8 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.1",
