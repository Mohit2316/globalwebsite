    "css-loader": "^0.25.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.9.0",
    "jsdom": "^9.9.1",
